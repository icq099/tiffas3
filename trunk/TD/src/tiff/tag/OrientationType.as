package tiff.tag
{
	public class OrientationType
	{
	public static const TOPLEFT:int		=1;	/* row 0 top, col 0 lhs */
	public static const TOPRIGHT:int	=2;	/* row 0 top, col 0 rhs */
	public static const BOTRIGHT:int	=3;	/* row 0 bottom, col 0 rhs */
	public static const BOTLEFT:int		=4;	/* row 0 bottom, col 0 lhs */
	public static const LEFTTOP:int		=5;	/* row 0 lhs, col 0 top */
	public static const RIGHTTOP:int	=6;	/* row 0 rhs, col 0 top */
	public static const RIGHTBOT:int	=7;	/* row 0 rhs, col 0 bottom */
	public static const LEFTBOT:int		=8;	/* row 0 lhs, col 0 bottom */
	}
}